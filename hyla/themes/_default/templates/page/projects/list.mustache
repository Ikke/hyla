<h1>Projects</h1>
<p><a href="{{create_url}}">Create a new project</a></p>
{{#has_projects}}
	<ul>
	{{#projects}}
		<li>
			<h2><a href="{{url}}">{{name}}</a></h2>
			<div>{{description}}</div>
		</li>
	{{/projects}}
	</ul>
{{/has_projects}}
{{^has_projects}}
	<p>No projects found.</p>
{{/has_projects}}